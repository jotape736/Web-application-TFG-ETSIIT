@misc{article_1445178,
	title={A simulated annealing algorithm for the faculty-level university course timetabling problem},
	journal={Pamukkale Üniversitesi Mühendislik Bilimleri Dergisi},
	volume={30},
	pages={17–34},
	year={2024},
	author={Erdoğan Akbulut, Hatice and Özçelik, Feriştah and Saraç, Tuğba},
	keywords={Faculty-level university course timetabling, Simulated annealing, Goal programming, Double major program, Minor program},
	abstract={In this study, faculty-level university course timetabling problem with double major and minor program constraints where classrooms are shared with several faculties is taken into account. This is the first study considering all these constraints together. A goal programming model is proposed to solve the considered problem. Since it is not possible to find a feasible solution for large-size problems with the proposed model in a time limit, a simulated annealing algorithm is developed. The performance of the proposed solution methods is tested by using randomly generated test problems. In addition, a case study is performed at the engineering faculty of a private university. Computational results show the success of the proposed simulated annealing algorithm to solve large-sized problems. An 83% improvement was achieved with the proposed algorithm for the real-life problem.},
	number={1},
	publisher={Pamukkale University},
	howpublished = {\url{https://dergipark.org.tr/en/pub/pajes/issue/83370/1445178#article_cite}}
}

@misc{doi:10.1504/IJOR.2024.139234,
	author = {Agrawal, Anil Kumar and Yadav, Susheel and Gupta, Amit Ambar and Pandey, Shubhendu},
	title = {Examination and class timetabling problem: a case study of an Indian university},
	journal = {International Journal of Operational Research},
	volume = {50},
	number = {3},
	pages = {291-315},
	year = {2024},
	doi = {10.1504/IJOR.2024.139234},
	howpublished = {\url{https://www.inderscienceonline.com/doi/abs/10.1504/IJOR.2024.139234}},
	abstract = {Timetabling problem is basically an optimisation problem and is a subset of scheduling problems. A timetabling problem is a problem where events (classes, examination and student) have to be ordered in time slots while satisfying some basic constraints. Here the basic aim is to satisfy various constraints, such as, no student can be scheduled for more than one course at the same time and the maximum number of students that can be scheduled to any particular class must not exceed the sitting capacity of the class. So, the timetabling problem is basically arranging the given set of values (class, subject and students) in such a manner such that the resulting schedule should fulfil all the above stated hard constraints and try to give more close values near to the soft constraints (such as proper distribution of classes over available slots). In the present work, examination timetable and class timetable problems have been considered with several practical features. The problems are formulated as mathematical models and heuristic approach is also proposed to solve them. The heuristic uses the basic framework of genetic algorithm.}
}

@article{Paladines,
